accessWidener	v1	named

# For removing nbt automatically written to shulker box items
accessible field net/minecraft/item/BlockItem BLOCK_ENTITY_TAG_KEY Ljava/lang/String;

# For classes implementing DualSpriteVertexConsumer to modify SpriteTexturedVertexConsumer.sprite
accessible field net/minecraft/client/render/SpriteTexturedVertexConsumer sprite Lnet/minecraft/client/texture/Sprite;
mutable field net/minecraft/client/render/SpriteTexturedVertexConsumer sprite Lnet/minecraft/client/texture/Sprite;

# For setting custom shulker box blocks' blockstate
mutable field net/minecraft/block/Block stateManager Lnet/minecraft/state/StateManager;
